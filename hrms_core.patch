@@ -0,0 +1,40 @@
+import frappe
+
+def create_employee_ctc_assignment(doc, method):
+    """Triggered after Job Offer submission"""
+    try:
+        frappe.logger().info(f"ðŸ”¥ Hook triggered for Job Offer: {doc.name}")
+
+        # Avoid duplicate CTC record for this offer
+        if frappe.db.exists("Employee CTC Assignment", {"offer_reference": doc.name}):
+            frappe.logger().info(f"âš ï¸ CTC already exists for {doc.name}")
+            return
+
+        ctc_type = "Job Applicant" if doc.job_applicant else "Employee"
+        frappe.logger().info(f"CTC type detected: {ctc_type}")
+
+        ctc_doc = frappe.new_doc("Employee CTC Assignment")
+        ctc_doc.type = ctc_type
+        if ctc_type == "Job Applicant":
+            ctc_doc.applicant_email_address = doc.applicant_email
+            ctc_doc.job_applicant = doc.job_applicant
+        else:
+            ctc_doc.employee_id = frappe.db.get_value("Employee", {"job_offer": doc.name}, "name")
+
+        # Map fields
+        ctc_doc.ctc = doc.get("custom_ctc")
+        ctc_doc.variable = doc.get("custom_variable_pay")
+        ctc_doc.variable_pay_frequency = None
+        ctc_doc.offer_reference = doc.name
+        ctc_doc.effective_from = doc.offer_date
+        ctc_doc.company = doc.company
+        ctc_doc.designation = doc.designation
+
+        frappe.logger().info(f"ðŸš€ Inserting CTC Assignment for {doc.name}")
+        ctc_doc.insert(ignore_permissions=True)
+        frappe.db.commit()
+        frappe.logger().info(f"âœ… CTC Assignment created for Job Offer {doc.name}")
+
+    except Exception:
+        frappe.log_error(frappe.get_traceback(), f"Job Offer â†’ Employee CTC Assignment Error for {doc.name}")
+        raise
