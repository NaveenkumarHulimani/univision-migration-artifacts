+import frappe
+from frappe import _
+
+@frappe.whitelist()
+def get_job_applicant_by_email(email):
+    if not email:
+        frappe.throw(_("Email is required"))
+
+    applicant = frappe.get_all(
+        "Job Applicant",
+        filters={"email_id": email},
+        fields=[
+            "name",
+            "applicant_name", "email_id", "phone_number",
+            "country", "job_title", "designation",
+            "status", "source", "source_name",
+            "employee_referral", "applicant_rating",
+            "notes", "cover_letter",
+            "resume_attachment", "resume_link",
+            "currency", "lower_range", "upper_range"
+        ],
+        limit_page_length=1
+    )
+
+    return applicant[0] if applicant else {}
+
+
+@frappe.whitelist()
+def save_job_applicant(data):
+    import json
+    data = json.loads(data) if isinstance(data, str) else data
+
+    email = data.get("email_id")
+    if not email:
+        frappe.throw(_("Email ID missing"))
+
+    # Get the correct document using email
+    applicant = frappe.get_all(
+        "Job Applicant",
+        filters={"email_id": email},
+        fields=["name"],
+        limit_page_length=1
+    )
+
+    if applicant:
+        doc = frappe.get_doc("Job Applicant", applicant[0].name)
+        doc.update(data)
+        doc.save(ignore_permissions=True)
+        frappe.db.commit()
+        return {"status": "updated", "name": doc.name}
+
+    # Create new applicant
+    doc = frappe.get_doc({"doctype": "Job Applicant", **data})
+    doc.insert(ignore_permissions=True)
+    frappe.db.commit()
+    return {"status": "created", "name": doc.name}
