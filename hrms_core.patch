@@ -0,0 +1,87 @@
+import frappe
+from frappe import _
+
+def execute(filters=None):
+    if not filters:
+        filters = {}
+    filters = frappe._dict(filters)
+
+    columns = get_columns()
+    data = get_data(filters)
+
+    return columns, data
+
+
+def get_columns():
+    return [
+        {"label": _("Job Opening"), "fieldname": "job_opening", "fieldtype": "Link", "options": "Job Opening", "width": 150},
+        {"label": _("Job Title"), "fieldname": "job_title", "fieldtype": "Data", "width": 200},
+        {"label": _("Status"), "fieldname": "status", "fieldtype": "Data", "width": 100},
+        {"label": _("Positions"), "fieldname": "number_of_positions", "fieldtype": "Int", "width": 100},
+        {"label": _("Total Applicants"), "fieldname": "total_applicants", "fieldtype": "Int", "width": 120},
+        {"label": _("Created On"), "fieldname": "creation", "fieldtype": "Datetime", "width": 150},
+        {"label": _("Last Modified"), "fieldname": "modified", "fieldtype": "Datetime", "width": 150}
+    ]
+
+
+def get_data(filters):
+    data = []
+    job_openings = get_job_openings(filters)
+
+    # Get applicants mapped by job_title (linking to Job Opening)
+    jo_ja_map = get_applicants([jo.name for jo in job_openings])
+
+    for jo in job_openings:
+        row = {
+            "job_opening": jo.name,
+            "job_title": jo.job_title,
+            "status": jo.status,
+            "number_of_positions": jo.number_of_positions or 0,
+            "total_applicants": len(jo_ja_map.get(jo.name, [])),
+            "creation": jo.creation,
+            "modified": jo.modified
+        }
+        data.append(row)
+
+    return data
+
+
+def get_job_openings(filters):
+    conditions = [["company", "=", filters.company]]
+
+    if filters.get("status"):
+        conditions.append(["status", "=", filters.status])
+    if filters.get("creation_from"):
+        conditions.append(["creation", ">=", filters.creation_from])
+    if filters.get("creation_to"):
+        conditions.append(["creation", "<=", filters.creation_to])
+    if filters.get("job_title"):
+        conditions.append(["job_title", "like", f"%{filters.job_title}%"])
+
+    job_openings = frappe.get_all(
+        "Job Opening",
+        filters=conditions,
+        fields=["name", "job_title", "status", "planned_vacancies as number_of_positions", "creation", "modified"],
+        order_by="creation desc",
+        limit=100
+    )
+
+    return job_openings
+
+
+def get_applicants(jo_list):
+    jo_ja_map = {}
+    if not jo_list:
+        return jo_ja_map
+
+    # Map applicants using job_title (link to Job Opening)
+    applicants = frappe.get_all(
+        "Job Applicant",
+        filters=[["job_title", "in", jo_list]],
+        fields=["name", "job_title", "applicant_name", "status"]
+    )
+
+    for ja in applicants:
+        jo_ja_map.setdefault(ja.job_title, []).append(ja)
+
+    return jo_ja_map
